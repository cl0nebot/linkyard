<h1>Edit Interaction</h1>
<%= form_for @interaction, :url => interaction_path do |f| %>
  <%= render "shared/errors", locals: { errors?: @interaction.errors.any?, error_message: @interaction.errors.full_messages.to_sentence } %>
  <%= render @interaction.type.underscore %>
  <%= f.submit 'Update' %>
<% end %>
